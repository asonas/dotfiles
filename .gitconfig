[user]
    name = asonas
    email = hzw1258@gmail.com
[core]
    excludesfile = ~/.gitignore

[alias]
    graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
    st = status
    df = diff
    dfc = diff --cached
    co = checkout
    last = show -1 HEAD --stat -p
    me = log --author=asonas
    ci = commit
    bull = pull
    dfw = diff --color-words
    b = branch
    log = log --decorate
    l = log --decorate

[color]
    ui = true
    diff = true

