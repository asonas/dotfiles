[user]
    name = asonas
    email = hzw1258@gmail.com
[core]
    excludesfile = ~/.gitignore

[alias]
    graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short
    st = status
    df = diff
    co = checkout
    last = show -1 HEAD --stat -p
    me = log --author=asonas
    ci = commit
    bull = pull
    ore = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%an] %Cgreen%d%Creset %s\" --all --date=short --author=fujiwara
    dfc = diff --color-words

[color]
    ui = true
    diff = true

